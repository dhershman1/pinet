{
  "name": "pinet",
  "version": "1.1.3",
  "description": "Yet another jsdocs template module",
  "main": "index.js",
  "scripts": {
    "test": "run-p test:*",
    "test:tmpl": "tape tests/tmpl/*.js | tap-on",
    "test:engine": "tape tests/engine.js | tap-on",
    "docs": "node_modules/.bin/jsdoc -c jsdoc.json",
    "lint": "standard | snazzy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhershman1/pinet.git"
  },
  "keywords": [
    "jsdocs",
    "template",
    "pinet",
    "documentation",
    "docs"
  ],
  "author": "Dustin Hershman <dustinh17@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dhershman1/pinet/issues"
  },
  "homepage": "https://github.com/dhershman1/pinet#readme",
  "dependencies": {
    "fs-extra": "11.1.0",
    "highlight.js": "11.7.0",
    "kyanite": "1.6.3",
    "marked": "4.2.12"
  },
  "devDependencies": {
    "jsdoc": "4.0.0",
    "npm-run-all": "4.1.5",
    "snazzy": "9.0.0",
    "standard": "17.0.0",
    "tap-on": "0.3.1",
    "tape": "5.6.3"
  },
  "standard": {
    "ignore": [
      "docs/",
      "example/"
    ]
  }
}

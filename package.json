{
  "name": "pinet",
  "version": "1.1.2",
  "description": "Yet another jsdocs template module",
  "main": "index.js",
  "scripts": {
    "test": "run-p test:*",
    "test:tmpl": "tape tests/tmpl/*.js | tap-spec",
    "test:engine": "tape tests/engine.js | tap-spec",
    "docs": "node_modules/.bin/jsdoc -c jsdoc.json",
    "lint": "standard | snazzy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhershman1/pinet.git"
  },
  "keywords": [
    "jsdocs",
    "template",
    "pinet",
    "documentation",
    "docs"
  ],
  "author": "Dustin Hershman <dustinh17@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dhershman1/pinet/issues"
  },
  "homepage": "https://github.com/dhershman1/pinet#readme",
  "dependencies": {
    "fs-extra": "9.1.0",
    "highlight.js": "10.6.0",
    "kyanite": "1.5.0",
    "marked": "2.0.0"
  },
  "devDependencies": {
    "jsdoc": "3.6.6",
    "npm-run-all": "4.1.5",
    "snazzy": "9.0.0",
    "standard": "16.0.3",
    "tap-spec": "5.0.0",
    "tape": "5.1.1"
  },
  "standard": {
    "ignore": [
      "docs/",
      "example/"
    ]
  }
}

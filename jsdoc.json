{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "includePattern": ".js$",
    "include": [
      "example/fixture",
      "./README.md"
    ]
  },
  "sourceType": "module",
  "plugins": [
    "example/custom-tags/index.js"
  ],
  "opts": {
    "encoding": "utf8",
    "template": "./",
    "destination": "./docs/",
    "recurse": true,
    "verbose": true
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": false,
    "idInHeadings": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true,
      "includeDate": false
    }
  },
  "pinet": {
    "title": "Testing -- Documentation",
    "lang": "en",
    "customTags": ["category"],
    "meta": [
      {
        "name": "viewport",
        "content": "width=device-width, initial-scale=1.0"
      }
    ]
  }
}

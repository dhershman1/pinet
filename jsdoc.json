{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "includePattern": ".js$",
    "include": [
      "node_modules/kyanite/src",
      "./README.md"
    ]
  },
  "sourceType": "module",
  "plugins": [
    "tags.js"
  ],
  "opts": {
    "encoding": "utf8",
    "template": "./",
    "destination": "./docs/",
    "recurse": true,
    "verbose": false
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": false,
    "idInHeadings": true
  },
  "pinet": {
    "title": "Testing -- Documentation",
    "packageJson": "kyanite/package.json",
    "links": [
      {
        "name": "Github",
        "link": "https://github.com/dhershman1/kyanite"
      }
    ],
    "lang": "en",
    "customTags": ["category", "signature"],
    "meta": [
      {
        "name": "viewport",
        "content": "width=device-width, initial-scale=1.0"
      },
      {
        "charset": "UTF-8"
      }
    ]
  }
}
